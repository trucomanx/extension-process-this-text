{
  "name": "process-this-text",
  "displayName": "process this text",
  "description": "An extension to process text and show diff",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:TrucomanX.processText",
    "onCommand:TrucomanX.replaceText" 
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "TrucomanX.processText",
        "title": "TrucomanX: Process Selected Text"
      },
      {
        "command": "TrucomanX.replaceText", 
        "title": "TrucomanX: Replace this text"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
